{
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/supertest": "^2.0.12"
  },
  "scripts": {
    "test": "jest --testEnvironment=node --watchAll --verbose --maxWorkers=1 ./dist/tests/integration",
    "build": "tsc -b",
    "start": "tsc -b && nodemon ./dist/app.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run format",
      "git add --all"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "files/*"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jest": "^25.1.0",
    "mongoose": "^6.3.1",
    "multer": "^1.4.4",
    "path": "^0.12.7",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13"
  }
}
